{% render 'partials/head.liquid' %}
<section>

   {% for product in data %}
            <li>
                <h2>{{ product.title }}</h2>
                <p>{{ product.description }}</p>
                <img src="{{ product.image }}" alt="{{ product.title }}">
            </li>
        {% endfor %}
</section>

{% render 'partials/foot.liquid' %}